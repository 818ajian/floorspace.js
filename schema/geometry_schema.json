{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"version": "0.4.0",
	"description": "Comment describing your JSON Schema",
	"type": "object",
	"properties": {
		"version": {
			"description": "File format version",
			"type": "string"
		},
		"application": {
			"$ref": "#/definitions/Application"
		},
		"project": {
			"$ref": "#/definitions/Project"
		},
		"stories": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/Story"
			}
		},
		"building_units": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/BuildingUnit"
			}
		},
		"thermal_zones": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/ThermalZone"
			}
		},
		"space_types": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/SpaceType"
			}
		},
		"construction_sets": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/ConstructionSet"
			}
		},
		"window_definitions": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/WindowDefinition"
			}
		},
		"daylighting_control_definitions": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/DaylightingControlDefinition"
			}
		},
		"pitched_roofs": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/PitchedRoof"
			}
		}
	},
	"required": [
		"version",
		"application",
		"project",
		"stories",
		"building_units",
		"thermal_zones",
		"space_types",
		"construction_sets",
		"window_definitions",
		"daylighting_control_definitions",
		"pitched_roofs"
	],
	"definitions": {
		"Application": {
			"description": "Reserved for application data, not for external use",
			"type": "object"
		},
		"Project": {
			"description": "Information that applies to the entire project",
			"type": "object",
			"properties": {
				"config": {
					"type": "object",
					"properties": {
						"units": {
							"type": [
								"string",
								"null"
							],
							"enum": [
								"ft",
								"m"
							],
							"default": "m"
						},
						"language": {
							"type": [
								"string",
								"null"
							],
							"default": "EN-US"
						}
					},
					"required": [
						"units",
						"language"
					]
				},
				"north_axis": {
					"description": "Angle between building north and true north in degrees (clockwise is positive)",
					"type": [
						"number",
						"null"
					],
					"default": 0
				},
				"default_wwr": {
					"description": "Default Window to Wall Ratio settings",
					"type": "object",
					"properties": {
						"apply_default_wwr_to": {
							"description": "Describes which surfaces the window to wall ratios will be applied to.  If \"All exterior surfaces\" then individual windows will be removed.  If \"Exterior surfaces with no windows\" then only surfaces without explicit windows will be modified to have the default wwr. If \"No exterior surfaces\" then the default wwr values will not be applied to any surfaces.",
							"type": [
								"string",
								"null"
							],
							"enum": [
								"All exterior surfaces",
								"Exterior surfaces with no windows",
								"No exterior surfaces"
							],
							"default": "Exterior surfaces with no windows"
						},
						"north_wwr": {
							"description": "Default wwr to apply, if null then no wwr will be applied",
							"type": [
								"number",
								"null"
							],
							"minimum": 0,
							"maximum": 0.99
						},
						"east_wwr": {
							"description": "Default wwr to apply, if null then no wwr will be applied",
							"type": [
								"number",
								"null"
							],
							"minimum": 0,
							"maximum": 0.99
						},
						"south_wwr": {
							"description": "Default wwr to apply, if null then no wwr will be applied",
							"type": [
								"number",
								"null"
							],
							"minimum": 0,
							"maximum": 0.99
						},
						"west_wwr": {
							"description": "Default wwr to apply, if null then no wwr will be applied",
							"type": [
								"number",
								"null"
							],
							"minimum": 0,
							"maximum": 0.99
						}
					},
					"required": [
						"apply_default_wwr_to",
						"north_wwr",
						"east_wwr",
						"south_wwr",
						"west_wwr"
					]
				},
				"ground": {
					"type": "object",
					"properties": {
						"floor_offset": {
							"description": "Specifies the vertical offset between the building floor and the ground plane. Use a value < 0 for buildings with below-grade spaces, or a value > 0 for, e.g., buildings on piers.",
							"type": [
								"number",
								"null"
							],
							"default": 0
						},
						"azimuth_angle": {
							"description": "Direction of the ground plane, in clockwise degrees from the positive y-axis when viewed from above.",
							"type": [
								"number",
								"null"
							],
							"default": 0
						},
						"tilt_slope": {
							"description": "Slope of the ground plane from horizontal. For example, a value of 1 implies a 45-deg angle.",
							"type": [
								"number",
								"null"
							],
							"default": 0
						}
					},
					"required": [
						"floor_offset",
						"azimuth_angle",
						"tilt_slope"
					]
				},
				"grid": {
					"type": "object",
					"properties": {
						"visible": {
							"type": [
								"boolean",
								"null"
							]
						},
						"spacing": {
							"type": [
								"number",
								"null"
							]
						}
					},
					"required": [
						"visible",
						"spacing"
					]
				},
				"view": {
					"type": "object",
					"properties": {
						"min_x": {
							"type": [
								"number",
								"null"
							]
						},
						"min_y": {
							"type": [
								"number",
								"null"
							]
						},
						"max_x": {
							"type": [
								"number",
								"null"
							]
						},
						"max_y": {
							"type": [
								"number",
								"null"
							]
						}
					},
					"required": [
						"min_x",
						"min_y",
						"max_x",
						"max_y"
					]
				},
				"map": {
					"type": "object",
					"properties": {
						"visible": {
							"type": [
								"boolean",
								"null"
							]
						},
						"latitude": {
							"type": [
								"number",
								"null"
							]
						},
						"longitude": {
							"type": [
								"number",
								"null"
							]
						},
						"zoom": {
							"type": [
								"number",
								"null"
							]
						},
						"elevation": {
							"type": [
								"number",
								"null"
							]
						},
						"enabled": {
							"type": [
								"boolean",
								"null"
							]
						},
						"initialized": {
							"type": [
								"boolean",
								"null"
							]
						},
						"rotation": {
							"type": [
								"number",
								"null"
							]
						}
					},
					"required": [
						"visible",
						"latitude",
						"longitude",
						"zoom",
						"elevation",
						"enabled",
						"initialized",
						"rotation"
					]
				},
				"previous_story": {
					"type": "object",
					"properties": {
						"visible": {
							"type": [
								"boolean",
								"null"
							]
						}
					},
					"required": [
						"visible"
					]
				},
				"show_import_export": {
					"type": [
						"boolean",
						"null"
					]
				}
			},
			"required": [
				"config",
				"north_axis",
				"default_wwr",
				"ground",
				"grid",
				"view",
				"map",
				"previous_story",
				"show_import_export"
			]
		},
		"Geometry": {
			"type": "object",
			"properties": {
				"id": {
					"type": "string"
				},
				"vertices": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/Vertex"
					}
				},
				"edges": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/Edge"
					}
				},
				"faces": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/Face"
					}
				}
			},
			"required": [
				"id",
				"vertices",
				"edges",
				"faces"
			]
		},
		"Vertex": {
			"type": "object",
			"properties": {
				"id": {
					"type": "string"
				},
				"x": {
					"type": "number"
				},
				"y": {
					"type": "number"
				},
				"edge_ids": {
					"type": "array",
					"items": {
						"type": "string"
					}
				}
			},
			"required": [
				"id",
				"x",
				"y",
				"edge_ids"
			]
		},
		"Edge": {
			"type": "object",
			"properties": {
				"id": {
					"type": "string"
				},
				"vertex_ids": {
					"type": "array",
					"minItems": 2,
					"maxItems": 2,
					"uniqueItems": true,
					"items": {
						"type": "string"
					}
				},
				"face_ids": {
					"type": "array",
					"minItems": 1,
					"maxItems": 2,
					"items": {
						"type": "string"
					}
				}
			},
			"required": [
				"id",
				"vertex_ids",
				"face_ids"
			]
		},
		"Face": {
			"type": "object",
			"properties": {
				"id": {
					"type": "string"
				},
				"edge_ids": {
					"type": "array",
					"minItems": 3,
					"items": {
						"type": "string"
					}
				},
				"edge_order": {
					"type": "array",
					"minItems": 3,
					"items": {
						"type": "number",
						"enum": [
							0,
							1
						]
					}
				}
			},
			"required": [
				"id",
				"edge_ids",
				"edge_order"
			]
		},
		"Story": {
			"type": "object",
			"properties": {
				"id": {
					"type": "string"
				},
				"handle": {
					"type": [
						"string",
						"null"
					]
				},
				"name": {
					"type": [
						"string",
						"null"
					]
				},
				"image_visible": {
					"type": [
						"boolean",
						"null"
					]
				},
				"below_floor_plenum_height": {
					"type": [
						"number",
						"null"
					],
					"minimum": 0
				},
				"floor_to_ceiling_height": {
					"type": [
						"number",
						"null"
					],
					"minimum": 0
				},
				"above_ceiling_plenum_height": {
					"type": [
						"number",
						"null"
					],
					"minimum": 0
				},
				"multiplier": {
					"type": [
						"integer",
						"null"
					],
					"minimum": 1,
					"multipleOf": 1,
					"default": 1
				},
				"color": {
					"type": [
						"string",
						"null"
					]
				},
				"geometry": {
					"$ref": "#/definitions/Geometry"
				},
				"images": {
					"type": "array"
				},
				"spaces": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/Space"
					}
				},
				"shading": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/Shading"
					}
				},
				"windows": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"window_definition_id": {
								"type": "string"
							},
							"edge_id": {
								"type": "string"
							},
							"alpha": {
								"description": "Distance along edge to center point of window, starting from first vertex of edge at a = 0 and ending at second vertex of edge at a =1.  v = v1 + a*(v2-v1)",
								"type": "number",
								"minimum": 0,
								"maximum": 1
							}
						},
						"required": [
							"window_definition_id",
							"edge_id",
							"alpha"
						]
					}
				}
			},
			"required": [
				"id",
				"handle",
				"name",
				"image_visible",
				"below_floor_plenum_height",
				"floor_to_ceiling_height",
				"above_ceiling_plenum_height",
				"multiplier",
				"color",
				"geometry",
				"images",
				"spaces",
				"shading",
				"windows"
			]
		},
		"Space": {
			"type": "object",
			"properties": {
				"id": {
					"type": "string"
				},
				"handle": {
					"type": [
						"string",
						"null"
					]
				},
				"name": {
					"type": [
						"string",
						"null"
					]
				},
				"face_id": {
					"type": [
						"string",
						"null"
					]
				},
				"building_unit_id": {
					"type": [
						"string",
						"null"
					]
				},
				"thermal_zone_id": {
					"type": [
						"string",
						"null"
					]
				},
				"space_type_id": {
					"type": [
						"string",
						"null"
					]
				},
				"construction_set_id": {
					"type": [
						"string",
						"null"
					]
				},
				"pitched_roof_id": {
					"type": [
						"string",
						"null"
					]
				},
				"daylighting_controls": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"daylighting_control_definition_id": {
								"type": "string"
							},
							"vertex_id": {
								"type": "string"
							}
						},
						"required": [
							"daylighting_control_definition_id",
							"vertex_id"
						]
					}
				},
				"below_floor_plenum_height": {
					"description": "If provided, overrides the story below_floor_plenum_height.",
					"type": [
						"number",
						"null"
					],
					"minimum": 0
				},
				"floor_to_ceiling_height": {
					"description": "If provided, overrides the story floor_to_ceiling_height.",
					"type": [
						"number",
						"null"
					],
					"minimum": 0
				},
				"above_ceiling_plenum_height": {
					"description": "If provided, overrides the story above_ceiling_plenum_height.",
					"type": [
						"number",
						"null"
					],
					"minimum": 0
				},
				"floor_offset": {
					"description": "If provided, specifies the vertical offset between the space floor and the story floor.",
					"type": [
						"number",
						"null"
					]
				},
				"open_to_below": {
					"description": "If true, defines a space that is open to below (has no floor).",
					"type": [
						"boolean",
						"null"
					]
				}
			},
			"required": [
				"id",
				"handle",
				"name",
				"face_id",
				"building_unit_id",
				"thermal_zone_id",
				"space_type_id",
				"construction_set_id",
				"pitched_roof_id",
				"daylighting_controls",
				"below_floor_plenum_height",
				"floor_to_ceiling_height",
				"above_ceiling_plenum_height",
				"floor_offset",
				"open_to_below"
			]
		},
